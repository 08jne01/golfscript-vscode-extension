{
	"name": "mock-debug",
	"version": "0.1.1",
	"publisher": "vscode",
	"description": "Starter extension for developing debug adapters for VSCode.",
	"engines": { "vscode": "*" },
	"contributes": {
		"debugAdapter": [{
			"type": "mock",
			"enableBreakpointsFor": { "languageIds": ["markdown"] },
			"program": "./debugAdapter/mock/openDebugMock.js",
			"runtime": "node"
		}]
	}
}